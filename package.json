{
  "name": "static-blog-generator-hexo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "prestart": "run-s build:chimera copy",
    "start": "yarn server",
    "build": "run-s build:*",
    "build:chimera": "cd multisite/chimeraland && yarn build",
    "build:post": "yarn copy",
    "build:site": "hexo generate",
    "copy": "sbg post copy",
    "clean": "gulp clean",
    "deploy": "gulp deploy",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:chimeraland": "cd multisite/chimeraland/chimeraland && yarn run update",
    "update:chimeraland2": "cd multisite/chimeraland && yarn run update",
    "update:ncu": "npm-check-updates -u",
    "update": "run-s update:* && npm install",
    "postupdate": "node postinstall.js --commit --yarn"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "@next-theme/plugins": "^8.14.2",
    "cheerio": "^0.22.0",
    "css": "^3.0.0",
    "hexo": "6.3.0",
    "hexo-asset-link": "^2.2.1",
    "hexo-browsersync": "https://github.com/hexojs/hexo-browsersync/tarball/master",
    "hexo-clean-css": "^2.0.0",
    "hexo-excerpt": "^1.3.0",
    "hexo-extend-injector2": "^0.3.1",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-fs": "^4.1.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-redirect": "./packages/hexo-generator-redirect",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-github-card": "./packages/hexo-github-card",
    "hexo-i18n": "^2.0.0",
    "hexo-pagination": "^3.0.0",
    "hexo-pdf": "^1.1.1",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-seo": "https://github.com/dimaslanjaka/hexo-seo/tarball/master",
    "hexo-server": "^3.0.0",
    "hexo-uglify": "^2.0.0",
    "hexo-util": "^3.0.1",
    "js-yaml": "^4.1.0",
    "lunr": "2.3.9",
    "sharp": "^0.31.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/monorepo/release/git-command-helper.tgz",
    "gulp": "^4.0.2",
    "hexo-theme-next": "./themes/next2",
    "instant-indexing": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/instant-indexing/release/instant-indexing.tgz",
    "lerna": "^6.5.1",
    "npm-run-all": "^4.1.5",
    "nx": "^15.7.2",
    "prettier": "^2.8.4",
    "safelinkify": "https://github.com/dimaslanjaka/safelink/raw/monorepo/release/safelinkify.tgz",
    "sbg-api": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-api/release/sbg-api.tgz",
