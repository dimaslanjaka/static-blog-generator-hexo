{
  "name": "static-blog-generator-hexo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "run-s build:chimera copy",
    "start": "npm run server",
    "build": "run-s build:**",
    "build:chimera": "cd multisite/chimeraland && npm run build",
    "build:post": "npm run copy",
    "build:site": "gulp build",
    "copy": "gulp copy-all-post",
    "clean": "gulp clean",
    "deploy": "npm run build && gulp deploy",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "preinstall": "node preinstall.js",
    "postinstall": "node postinstall.js",
    "preupdate": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update": "npx npm-check-updates -u -x hexo,chalk && npm install --cache tmp/npm",
    "refresh": "npm update hexo-front-matter hexo-browsersync hexo-generator-redirect static-blog-generator safelinkify hexo-post-parser"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "hexo": "^6.3.0",
    "hexo-asset-link": "^2.2.1",
    "hexo-browsersync": "https://github.com/hexojs/hexo-browsersync#master",
    "hexo-excerpt": "^1.3.0",
    "hexo-extend-injector2": "^0.3.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-redirect": "file:packages/hexo-generator-redirect",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-github-card": "file:packages/hexo-github-card",
    "hexo-i18n": "^2.0.0",
    "hexo-pagination": "^3.0.0",
    "hexo-pdf": "^1.1.1",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-seo": "https://github.com/dimaslanjaka/hexo-seo",
    "hexo-server": "^3.0.0",
    "hexo-theme-next": "file:themes/next",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "ansi-colors": "^4.1.3",
    "axios": "^1.2.2",
    "axios-cache-interceptor": "^1.0.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn#typescript",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper#master",
    "git-describe": "^4.1.1",
    "gulp": "^4.0.2",
    "hexo-post-parser": "https://github.com/dimaslanjaka/hexo-post-parser#master",
    "hpagent": "^1.2.0",
    "md5-file": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "persistent-cache": "https://github.com/dimaslanjaka/persistent-cache#improve2",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "safelinkify": "https://github.com/dimaslanjaka/safelink.git#master",
    "static-blog-generator": "https://github.com/dimaslanjaka/static-blog-generator#master",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
  }
}