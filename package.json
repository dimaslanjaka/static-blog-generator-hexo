{
  "name": "static-blog-generator-hexo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "run-s build:chimera copy",
    "start": "npm run server",
    "build": "run-s build:**",
    "build:chimera": "cd multisite/chimeraland && npm run build",
    "build:post": "npm run copy",
    "build:site": "gulp build",
    "copy": "gulp copy-all-post",
    "clean": "gulp clean",
    "deploy": "npm run build && gulp deploy",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:chimeraland": "cd multisite/chimeraland/chimeraland && npm run update",
    "update:chimeraland2": "cd multisite/chimeraland && npm run update",
    "update:sbg": "cd packages/static-blog-generator && npm run update && npm run build",
    "update:ncu": "npx npm-check-updates -u -x hexo,chalk",
    "update": "run-s update:** && npm install",
    "postinstall": "node postinstall.js --commit",
    "refresh": "rimraf package-lock.json && npm update hexo-front-matter hexo-browsersync hexo-generator-redirect static-blog-generator safelinkify hexo-post-parser"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "chimeraland": "file:multisite/chimeraland",
    "hexo": "^6.3.0",
    "hexo-asset-link": "^2.2.1",
    "hexo-browsersync": "https://github.com/hexojs/hexo-browsersync/tarball/master",
    "hexo-excerpt": "^1.3.0",
    "hexo-extend-injector2": "^0.3.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-redirect": "file:packages/hexo-generator-redirect",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-github-card": "file:packages/hexo-github-card",
    "hexo-i18n": "^2.0.0",
    "hexo-pagination": "^3.0.0",
    "hexo-pdf": "^1.1.1",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-seo": "https://github.com/dimaslanjaka/hexo-seo/tarball/master",
    "hexo-server": "^3.0.0",
    "hexo-theme-next": "file:themes/next"
  },
  "devDependencies": {
    "@types/hexo": "file:packages/@types/hexo",
    "@types/hexo-util": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/hexo-util/release/types-hexo-util.tgz",
    "@types/node": "^18.11.18",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/through2",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "ansi-colors": "^4.1.3",
    "axios": "^1.2.2",
    "axios-cache-interceptor": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.0",
    "git-describe": "^4.1.1",
    "gulp": "^4.0.2",
    "gulp-dom": "file:packages/gulp-dom",
    "hpagent": "^1.2.0",
    "md5-file": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "sitemap-crawler": "file:packages/sitemap-crawler",
    "static-blog-generator": "file:packages/static-blog-generator",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "upath": "^2.0.1",
    "yargs": "^17.6.2"
  },
  "optionalDependencies": {
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/tarball/typescript",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/master/release/git-command-helper.tgz",
    "gulp-dom": "https://github.com/dimaslanjaka/static-blog-generator-hexo/raw/master/packages/gulp-dom/release/gulp-dom.tgz",
    "hexo-post-parser": "https://github.com/dimaslanjaka/hexo-post-parser/raw/master/release/hexo-post-parser.tgz",
    "nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
    "persistent-cache": "https://github.com/dimaslanjaka/persistent-cache/raw/improve2/release/persistent-cache.tgz",
    "safelinkify": "https://github.com/dimaslanjaka/safelink/raw/master/release/safelinkify.tgz",
    "sitemap-crawler": "https://github.com/dimaslanjaka/static-blog-generator-hexo/raw/master/packages/sitemap-crawler/release/sitemap-crawler.tgz",
    "static-blog-generator": "file:packages/static-blog-generator",
    "termux-chimeraland": "file:multisite/chimeraland/chimeraland"
  }
}
