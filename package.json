{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "static-blog-generator-hexo",
  "version": "1.0.0",
  "private": true,
  "description": "www.webmanajemen.com",
  "main": "package-install.js",
  "scripts": {
    "start": "yarn workspace wmi run server",
    "clean": "yarn workspace wmi run clean",
    "prebuild": "yarn workspace wmi run prebuild",
    "build": "yarn workspace wmi run build",
    "postbuild": "yarn workspace wmi run postbuild",
    "update:ncu": "npm-check-updates -u && yarn cache clean",
    "postinstall": "yarn workspace hexo-monorepo run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator-hexo.git"
  },
  "keywords": [],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "dimaslanjaka",
    "url": "https://webmanajemen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator-hexo/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator-hexo#readme",
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "@types/hexo": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/hexo",
    "eslint": "^8.38.0",
    "minimist": "^1.2.8",
    "prettier": "^2.8.7",
    "upath": "^2.0.1"
  },
  "workspaces": {
    "packages": [
      "site",
      "site/multisite/chimeraland",
      "site/src-posts",
      "packages/hexo-theme-claudia",
      "packages/hexo"
    ],
    "nohoist": [
      "**/hexo-theme-claudia",
      "**/hexo-theme-claudia/**"
    ]
  },
  "resolutions": {
    "safelinkify": "https://github.com/dimaslanjaka/safelink/raw/monorepo/release/safelinkify.tgz",
    "sbg-api": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-api/release/sbg-api.tgz",
    "sbg-cli": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-cli/release/sbg-cli.tgz",
    "sbg-server": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-server/release/sbg-server.tgz",
    "sbg-utility": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-utility/release/sbg-utility.tgz",
    "sitemap-crawler": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sitemap-crawler/release/sitemap-crawler.tgz",
