{
  "name": "chimeraland",
  "version": "0.0.2",
  "private": true,
  "homepage": "https://www.webmanajemen.com/chimeraland/",
  "repository": {
    "url": "https://github.com/dimaslanjaka/chimeraland.git",
    "type": "git"
  },
  "files": [
    "postinstall.js",
    "packer.js"
  ],
  "scripts": {
    "start": "node -r ts-node/register src/manager.ts",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.cjs",
    "update:ncu": "npx npm-check-updates -u",
    "update": "run-s update:** && node packer.js --yarn",
    "build": "ts-node build.ts",
    "copy": "node test/copy.js",
    "deploy": "node deploy.js",
    "lint": "run-s lint:**",
    "lint:src": "npx eslint --fix src/**/*.{ts,tsx,js,json}",
    "lint:sync": "npx eslint --fix sync/**/*.{ts,tsx,js,json}",
    "pack": "node packer.cjs --yarn"
  },
