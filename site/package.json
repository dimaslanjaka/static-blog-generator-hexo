{
  "name": "wmi",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "prestart": "run-s build:prepare build:post",
    "start": "yarn server",
    "build:prepare": "node -r ts-node/register prebuild.ts",
    "build:post": "npm run post",
    "build:site": "hexo generate",
    "build": "run-s \"build:**\"",
    "generate:seo": "sbg deploy seo",
    "generate:safelink": "sbg deploy safelink",
    "generate:install": "cd .deploy_git && npm install --production --omit=dev",
    "generate:install2": "cd .deploy_git/page && npm install --production --omit=dev",
    "generate:feed": "sbg generate feed",
    "generate:sitemap": "sbg generate sitemap",
    "generate": "run-s \"generate:**\"",
    "generate:copy": "sbg deploy copy",
    "post:chimera": "cd multisite/chimeraland && yarn install && yarn run build",
    "post:standalone": "sbg post standalone",
    "post:copy": "sbg post copy",
    "post": "run-s \"post:**\"",
    "clean": "hexo clean && sbg clean post && sbg clean cache",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "update-updater": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/updater.js > updater.js",
    "update:chimeraland": "cd multisite/chimeraland/chimeraland && yarn run update",
    "update:chimeraland2": "cd multisite/chimeraland && yarn run update",
    "update:ncu": "npx npm-check-updates -u -x jsdom,deepmerge-ts --enginesNode",
    "update": "run-s \"update:**\"",
    "post-update": "node postinstall.js --commit --yarn"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "workspaces": [
    "themes/flowbite",
    "themes/claudia",
    "themes/butterfly"
  ],
  "dependencies": {
    "binary-collections": "^2.0.5",
    "bluebird": "^3.7.2",
    "cheerio": "^0.22.0",
    "cross-spawn": "^7.0.3",
    "css": "^3.0.0",
    "fs-extra": "^11.1.1",
    "git-command-helper": "^2.0.2",
    "hexo": "^7",
    "hexo-asset-link": "^2.2.3",
    "hexo-browsersync": "^0.3.0",
    "hexo-cli": "^4.3.2",
    "hexo-excerpt": "^1.3.1",
    "hexo-extend-injector2": "^0.3.1",
    "hexo-filter-flowchart": "^1.0.4",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-front-matter": "^4.2.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-redirect": "^1.4.0",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-i18n": "^2.0.0",
    "hexo-log": "^4.1.0",
    "hexo-pagination": "^3.0.0",
    "hexo-pdf": "^1.1.1",
    "hexo-post-parser": "^1.2.14",
    "hexo-renderers": "^2.0.2",
    "hexo-seo": "^1.3.0",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "^1.2.3",
    "hexo-util": "^3.3.0",
    "instant-indexing": "^1.0.4",
    "js-yaml": "^4.1.0",
    "lodash.toarray": "^4.4.0",
    "lunr": "2.3.9",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "moment-timezone": "^0.5.45",
    "nib": "^1.2.0",
    "safelinkify": "^1.2.2",
    "sbg-api": "^1.1.0",
    "sbg-cli": "^1.1.0",
    "sbg-server": "^1.1.0",
    "sbg-utility": "^1.1.6",
    "sharp": "^0.32.1",
    "static-blog-generator": "^3.0.1",
    "stylus": "^0.63.0",
    "upath": "^2.0.1",
    "warehouse": "^5.0.1",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/hexo": "^3.8.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp": "^5.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "sitemap-crawler": "^1.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14"
  }
}
