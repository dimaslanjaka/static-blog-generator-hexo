{
  "name": "wmi",
  "version": "3.0.0",
  "private": true,
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "prestart": "run-s build:chimera copy",
    "start": "yarn server",
    "prebuild": "yarn post:standalone && yarn post:copy",
    "build": "hexo generate",
    "postbuild": "yarn generate:copy && yarn generate:feed && yarn generate:sitemap && yarn generate:install && yarn generate:install2",
    "generate:copy": "sbg dp copy",
    "generate:feed": "sbg generate feed",
    "generate:sitemap": "sbg generate sitemap",
    "generate:install": "cd .deploy_git && npm install --production --omit=dev",
    "generate:install2": "cd .deploy_git/page && npm install --production --omit=dev",
    "post:chimera": "cd multisite/chimeraland && yarn install && yarn run build",
    "post:copy": "sbg post copy",
    "post:standalone": "sbg post standalone",
    "clean": "hexo clean && sbg clean post && sbg clean cache",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "update:updater": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/updater.js > updater.js",
    "update:chimeraland": "cd multisite/chimeraland/chimeraland && yarn run update",
    "update:chimeraland2": "cd multisite/chimeraland && yarn run update",
    "update:ncu": "npm-check-updates -u",
    "update": "run-s update:**",
    "postupdate": "node postinstall.js --commit --yarn"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "css": "^3.0.0",
    "hexo": "https://github.com/hexojs/hexo/tarball/master",
    "hexo-asset-link": "^2.2.1",
    "hexo-browsersync": "https://github.com/hexojs/hexo-browsersync/tarball/master",
    "hexo-excerpt": "^1.3.0",
    "hexo-extend-injector2": "^0.3.1",
    "hexo-filter-flowchart": "^1.0.4",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-redirect": "workspace:^",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-i18n": "^2.0.0",
    "hexo-pagination": "^3.0.0",
    "hexo-pdf": "^1.1.1",
    "hexo-renderer-markdown-it": "^7.0.0",
    "hexo-renderers": "workspace:^",
    "hexo-seo": "workspace:^",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "workspace:^",
    "hexo-util": "^3.0.1",
    "js-yaml": "^4.1.0",
    "lodash.toarray": "^4.4.0",
    "lunr": "2.3.9",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "sharp": "^0.31.3",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/monorepo/release/git-command-helper.tgz",
    "gulp": "^4.0.2",
    "instant-indexing": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/instant-indexing/release/instant-indexing.tgz",
    "lerna": "^6.5.1",
    "npm-run-all": "^4.1.5",
    "nx": "^15.8.1",
    "prettier": "^2.8.4",
    "safelinkify": "https://github.com/dimaslanjaka/safelink/raw/monorepo/release/safelinkify.tgz",
    "sbg-api": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-api/release/sbg-api.tgz",
    "sbg-cli": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-cli/release/sbg-cli.tgz",
    "sbg-server": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-server/release/sbg-server.tgz",
    "sbg-utility": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sbg-utility/release/sbg-utility.tgz",
    "sitemap-crawler": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/sitemap-crawler/release/sitemap-crawler.tgz",
    "static-blog-generator": "https://github.com/dimaslanjaka/static-blog-generator/raw/beta/packages/static-blog-generator/release/static-blog-generator.tgz",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "hexo-blogger-xml": "https://github.com/dimaslanjaka/hexo-blogger-xml/raw/monorepo/release/hexo-blogger-xml.tgz"
  }
}