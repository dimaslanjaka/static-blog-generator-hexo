{
  "name": "wmi",
  "version": "3.0.0",
  "private": true,
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "prestart": "run-s build:chimera copy",
    "start": "yarn server",
    "prebuild": "node prebuild.js && yarn post:standalone && yarn post:copy",
    "build": "hexo generate",
    "postbuild": "yarn generate:copy && yarn generate:feed && yarn generate:sitemap && yarn generate:install && yarn generate:install2",
    "generate:copy": "sbg deploy copy",
    "generate:feed": "sbg generate feed",
    "generate:sitemap": "sbg generate sitemap",
    "generate:install": "cd .deploy_git && npm install --production --omit=dev",
    "generate:install2": "cd .deploy_git/page && npm install --production --omit=dev",
    "post:chimera": "cd multisite/chimeraland && yarn install && yarn run build",
    "post:standalone": "sbg post standalone",
    "post:copy": "sbg post copy",
    "post": "run-s \"post:**\"",
    "clean": "hexo clean && sbg clean post && sbg clean cache",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "update-updater": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/updater.js > updater.js",
    "update:chimeraland": "cd multisite/chimeraland/chimeraland && yarn run update",
    "update:chimeraland2": "cd multisite/chimeraland && yarn run update",
    "update:ncu": "npx npm-check-updates -u",
    "update": "run-s \"update:**\"",
    "postupdate": "node postinstall.js --commit --yarn"
  },
  "hexo": {
    "version": "7.0.0-rc1"
  },
  "dependencies": {
    "binary-collections": "^1.0.3",
    "cheerio": "^0.22.0",
    "css": "^3.0.0",
    "git-command-helper": "^1.2.3",
    "hexo": "^7.0.0-rc1",
    "hexo-asset-link": "^2.2.1",
    "hexo-browsersync": "^0.3.0",
    "hexo-cli": "^4.3.1",
    "hexo-excerpt": "^1.3.0",
    "hexo-extend-injector2": "^0.3.1",
    "hexo-filter-flowchart": "^1.0.4",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-front-matter": "^4.1.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-redirect": "^1.4.0",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-i18n": "^2.0.0",
    "hexo-log": "^4.0.1",
    "hexo-pagination": "^3.0.0",
    "hexo-pdf": "^1.1.1",
    "hexo-renderers": "https://github.com/dimaslanjaka/hexo-renderers/tarball/be00f361ffb4e1728463a2c9a714cfeb7c83913e",
    "hexo-seo": "^1.2.3",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "^1.2.2",
    "hexo-theme-claudia": "workspace:^",
    "hexo-util": "^3.0.1",
    "instant-indexing": "^1.0.4",
    "js-yaml": "^4.1.0",
    "lodash.toarray": "^4.4.0",
    "lunr": "2.3.9",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "nib": "^1.2.0",
    "safelinkify": "^1.1.18",
    "sbg-api": "^1.1.0",
    "sbg-cli": "^1.1.0",
    "sbg-server": "^1.1.0",
    "sbg-utility": "^1.1.0",
    "sharp": "^0.32.1",
    "static-blog-generator": "^3.0.1",
    "stylus": "^0.59.0",
    "warehouse": "^5.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "prettier": "^2.8.8",
    "sitemap-crawler": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
