{
  "name": "wmi",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "prestart": "run-s build:prepare build:post",
    "start": "yarn server",
    "build:prepare": "node -r ts-node/register prebuild.ts && npx hexo-theme-flowbite",
    "build:post": "npm run post",
    "build:site": "hexo generate --expose-gc",
    "build": "run-s \"build:**\"",
    "generate:copy": "sbg deploy copy --expose-gc",
    "generate:seo": "sbg deploy seo",
    "generate:safelink": "sbg deploy safelink",
    "generate:install": "cd .deploy_git && npm install --production --omit=dev",
    "generate:install2": "cd .deploy_git/page && npm install --production --omit=dev",
    "generate:feed": "sbg generate feed",
    "generate:sitemap": "sbg generate sitemap",
    "generate": "run-s \"generate:**\"",
    "post:chimera": "cd multisite/chimeraland && yarn install && yarn run build",
    "post:standalone": "sbg post standalone --expose-gc",
    "post:copy": "sbg post copy --expose-gc",
    "post": "run-s \"post:**\"",
    "clean:site": "hexo clean",
    "clean:post": "sbg clean post",
    "clean:cache": "sbg clean cache",
    "clean": "run-s \"clean:**\"",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "site:safe": "hexo generate --safe --expose-gc",
    "update:chimeraland": "cd multisite/chimeraland/chimeraland && yarn run update",
    "update:chimeraland2": "cd multisite/chimeraland && yarn run update",
    "update:ncu": "npx npm-check-updates -u",
    "update": "run-s \"update:**\""
  },
  "hexo": {
    "version": "7.3.0"
  },
  "workspaces": [
    "themes/hexo-theme-flowbite"
  ],
  "dependencies": {
    "binary-collections": "^2.0.6",
    "bluebird": "^3.7.2",
    "cross-spawn": "^7.0.6",
    "fs-extra": "^11.3.0",
    "git-command-helper": "^2.0.2",
    "hexo": "^7.3.0",
    "hexo-adsense": "^2.0.0",
    "hexo-asset-link": "^2.2.3",
    "hexo-cli": "^4.3.2",
    "hexo-excerpt": "^1.3.1",
    "hexo-front-matter": "^4.2.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-redirect": "^1.4.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-pagination": "^4.0.0",
    "hexo-post-parser": "^3.0.1",
    "hexo-renderers": "^3.0.0",
    "hexo-seo": "^2.0.1",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "^1.2.4",
    "hexo-util": "^3.3.0",
    "sbg-api": "^1.1.1",
    "sbg-cli": "^1.1.0",
    "sbg-utility": "^2.0.9",
    "static-blog-generator": "^3.0.1",
    "upath": "^2.0.1",
    "warehouse": "^6.0.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "gulp": "^5.0.1",
    "gulp-cli": "^3.1.0",
    "hexo-theme-flowbite": "*",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=14"
  }
}
