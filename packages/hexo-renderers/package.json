{
  "packageManager": "yarn@3.4.1",
  "name": "hexo-renderers",
  "version": "1.0.4",
  "main": "dist/renderer-loader.js",
  "types": "dist/renderer-loader.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.prettierrc.json > .prettierrc.json && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js && npx npm-check-updates -u",
    "build": "tsc --build tsconfig.build.json && gulp && node packer.js --yarn"
  },
  "keywords": [
    "hexo",
    "hexo-renderer-ejs",
    "hexo-renderer-nunjucks",
    "hexo-renderer-pug",
    "hexo-renderer-stylus",
    "hexo-renderer-sass",
    "hexo-renderer-swig"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "url": "https://www.webmanajemen.com",
    "name": "dimaslanjaka"
  },
  "license": "MIT",
  "description": "All in one hexo renderers",
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/hexo": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/hexo",
    "@types/lodash.toarray": "^4.4.7",
    "@types/node": "^18.14.6",
    "@types/nunjucks": "^3.2.2",
    "@types/pug": "^2.0.6",
    "eslint": "^8.35.0",
    "gulp": "^4.0.2",
    "hexo": "^6.3.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "ejs": "^3.1.8",
    "fs-extra": "^11.1.0",
    "hexo-util": "^3.0.1",
    "lodash.toarray": "^4.4.0",
    "nunjucks": "^3.2.3",
    "pug": "^3.0.2",
    "sass": "^1.58.3",
    "sbg-utility": "^1.0.6",
    "stylus": "^0.59.0",
    "upath": "^2.0.1"
  }
}